{
  "name": "fcc-multiple-choice",
  "version": "0.0.1",
  "homepage": "https://luucamay.github.io/multiple-choice-questions",
  "private": true,
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.3",
    "sass": "^1.63.4"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "immutable": "^3.8.1",
    "prismjs": "^1.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-screen-size": "^1.0.1",
    "redux": "^3.6.0"
  },
  "scripts": {
    "build-css": "sass src:src",
    "watch-css": "npm run build-css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
